<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(data:image/jpeg;base64,);">
  <div class="container">
<h1 class="text-white text-center">THIRD EYE VIEWS</h1>
<p class="text-white text-center">Your Personal Watch List</p>
  <p class="text-center rounded"><%= link_to "Create a movie list", new_list_path, class: "btn btn-secondary" %></p>
  </div>
</div>
<div class="container py-4 me-3">
  <div class="d-flex align-items-center justify-content-between mb-3">
    <h3 class="mb-2 ms-4">Movie Lists</h3>
  </div>
<div class="container mt-3">
  <div class="cards">
<% @lists.each do |list| %>
  <% list.name %>
<div class="co-12 col-sm-6 col-md-4 mb-3">
        <%= link_to list_path(list), class:"text-decoration-none" do %>
          <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(data:image/jpeg;base64,);">
           <p class="text-white"> <%= list.name %> </p>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
